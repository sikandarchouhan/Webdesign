using System;
public class Armstrong
{
    public static void Main(string[] args)
    {
        int n,r,sum=0,temp;
        Console.Write("Enter the number : ");
        n=int.Parse(Console.ReadLine());
        temp=n;
        while(n>0)
        {
            r=n%10;
            sum=sum+(r*r*r);
            n=n/10;
        }
        if(temp==sum)
        {
            Console.Write(temp+" is an amrstrong number");
        }
        else
        {
            Console.Write(temp+" is not an amrstrong number");
        }
        
    }
}
